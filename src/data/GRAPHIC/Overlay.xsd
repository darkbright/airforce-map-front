    <xs:attribute name="rot" type="xs:float" use="optional" default="0.0" />
    <xs:attribute name="rad" type="xs:float" use="optional" default="0.0" />
  </xs:complexType>
  <!--

      ols_ellipse : 타원 부호 객체 (Ellipse symbol)
