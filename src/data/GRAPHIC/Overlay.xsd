    <xs:attribute name="rot" type="xs:float" use="optional" default="0.0" />
  </xs:complexType>
  <!--

      ols_arc : 원호 부호 객체 (Arc symbol)
  -->
  <xs:complexType name="type_ols_arc">
    <!-- rot : rotate - 회전값 -->
    <!-- alt : arc line type - 호 선 모양 -->
    <!-- aft : arc fill type - 호 채움 모양 -->
    <xs:attribute name="rot" type="xs:float" use="optional" default="0.0" />
